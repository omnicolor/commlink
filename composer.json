{
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "wyrihaximus/composer-update-bin-autoload-path": false,
            "icanhazstring/composer-unused": true
        }
    },
    "description": "Commlink RPG manager.",
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "omnicolor/commlink",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "jenssegers/mongodb": "^3.8",
        "laravel-discord/yasmin": "0.8.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.8",
        "pusher/pusher-php-server": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "icanhazstring/composer-unused": "^0.8",
        "laravel/breeze": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/larastan": "^2.0",
        "php-mock/php-mock-phpunit": "^2.6",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^9.3.3",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "all": [
            "@test",
            "@cs",
            "@phpstan"
        ],
        "coverage": "phpunit --coverage-html=public/coverage",
        "cs": "phpcs && php-cs-fixer fix --dry-run --config=php_cs.php -v --using-cache=no --diff",
        "phpstan": "phpstan analyze",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "phpunit"
    },
    "type": "project"
}
